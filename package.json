{
  "name": "cafeta-components",
  "version": "0.0.1",
  "description": "Stencil Component Starter",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/cafeta-components/cafeta-components.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "cross-env NODE_ENV=production stencil build --docs",
    "dev": "stencil build --dev --watch --docs",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "start": "npm-run-all --parallel dev storybook",
    "storybook": "start-storybook -p 9001 -c .storybook -s www",
    "prepare": "npm run build",
    "install": "npm run build"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@fullhuman/postcss-purgecss": "^2.0.5",
    "@stencil/core": "^1.8.4",
    "@stencil/postcss": "^1.0.1",
    "@stencil/sass": "^1.1.1",
    "@storybook/addon-knobs": "^5.3.9",
    "@storybook/addon-notes": "^5.3.8",
    "@storybook/addon-storysource": "^5.3.8",
    "@storybook/addon-viewport": "^5.3.8",
    "@storybook/html": "^5.3.8",
    "autoprefixer": "^9.7.4",
    "babel-loader": "^8.0.6",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^7.0.0",
    "cssnano": "^4.1.10",
    "npm-run-all": "^4.1.5",
    "write-file-webpack-plugin": "^4.5.1"
  },
  "license": "MIT",
  "dependencies": {
    "tailwindcss": "^1.1.4"
  }
}
