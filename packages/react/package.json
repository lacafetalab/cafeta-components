{
  "name": "@cafeta/components-react",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "version": "1.3.12",
  "description": "React specific wrapper for cafeta-components",
  "repository": {
    "type": "git",
    "url": "https://github.com/lacafetalab/cafeta-components.git"
  },
  "scripts": {
    "prepublishOnly": "node ./scripts/copyFiles.js",
    "postpublish": "node ./scripts/removeFiles.js",
    "build": "npm run clean && npm run compile",
    "clean": "rm -rf dist",
    "compile": "npm run tsc",
    "tsc": "tsc -p .",
    "rollup": "rollup -c"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/",
    "tailwind.config.js",
    "global.css"
  ],
  "devDependencies": {
    "@types/jest": "23.3.9",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "jest": "^23.0.0",
    "jest-dom": "^3.0.2",
    "np": "^3.1.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "typescript": "~3.7.2"
  },
  "dependencies": {
    "@cafeta/components": "^1.2.11"
  },
  "peerDependencies": {
    "react": "^16.7.0",
    "react-dom": "^16.7.0"
  },
  "jest": {
    "preset": "ts-jest",
    "setupTestFrameworkScriptFile": "<rootDir>/jest.setup.js",
    "testPathIgnorePatterns": [
      "node_modules",
      "dist"
    ]
  },
  "license": "MIT",
  "gitHead": "3bf1aa844aff06c6296ca746c9ddf3a3e0009cc0"
}
